{
  "author": "RFS-0",
  "bugs": {
    "url": "https://github.com/RFS-0/material.resrc.ch/issues"
  },
  "description": "A SolidJS Port of Material 3",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "turbo": "^1.10.7"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://github.com/RFS-0/material.resrc.ch#readme",
  "keywords": [
    "SolidJS",
    "Material 3",
    "TypeScript"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "style": "dist/style.css",
  "module": "dist/index.mjs",
  "name": "material.resrc.ch-root",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RFS-0/material.resrc.ch.git"
  },
  "scripts": {
    "build": "turbo run build",
    "changeset": "changeset",
    "dev": "turbo run dev",
    "clean": "turbo run clean",
    "release": "changeset publish",
    "test": "turbo run test",
    "version": "changeset version && pnpm i --lockfile-only"
  },
  "types": "dist/index.d.ts",
  "version": "0.0.1-alpha.10",
  "workspaces": [
    "apps/*",
    "libs/*"
  ]
}
